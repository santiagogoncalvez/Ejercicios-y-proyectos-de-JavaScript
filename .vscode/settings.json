{
    "editor.fontWeight": "bold",
    "editor.fontFamily": "Rubik ExtraLight, Fira Code Light, Consolas, 'Courier New', monospace",
    "editor.fontVariations": true,
    "editor.tabSize": 4,
    "editor.lineHeight": 22,
    "chat.editor.lineHeight": 0,
    "terminal.integrated.lineHeight": 1.1,
    "debug.console.lineHeight": 40,
    "editor.fontSize": 14,
    "debug.console.fontFamily": "Consolas",
    "debug.console.fontSize": 14,
    "terminal.integrated.fontWeight": "normal",
    "notebook.output.fontFamily": "Consolas",
    "terminal.integrated.letterSpacing": 1,
    "terminal.integrated.cursorStyle": "line",
    "terminal.integrated.cursorStyleInactive": "outline",
    "terminal.integrated.cursorWidth": 3,
    "terminal.integrated.altClickMovesCursor": true,
    "terminal.integrated.cursorBlinking": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true,
    "[javascript]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "prettier.tabWidth": 4,
    "editor.cursorWidth": 2,
    "prettier.printWidth": 80,
    "editor.fontLigatures": true,
    "editor.letterSpacing": 2
}
